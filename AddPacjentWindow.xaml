<Window x:Class="Przychodnia.AddPacjentWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Dodaj Pacjenta" Height="320" Width="400">
    <Grid>
        <StackPanel Margin="10">
            <TextBlock Text="Imię:" Margin="5"/>
            <TextBox Name="txtImie" Margin="5"/>
            <TextBlock Text="Nazwisko:" Margin="5"/>
            <TextBox Name="txtNazwisko" Margin="5"/>
            <TextBlock Text="PESEL:" Margin="5"/>
            <TextBox Name="txtPesel" Margin="5"/>
            <TextBlock Text="Numer telefonu:" Margin="5"/>
            <TextBox Name="txtTelefon" Margin="5"/>
            <Button Content="Dodaj" Click="btnDodaj_Click" Margin="10"/>
        </StackPanel>
    </Grid>
</Window>
