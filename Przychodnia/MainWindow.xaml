<Window x:Class="Przychodnia.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Przychodnia" Height="350" Width="525">
    <Grid>
        <StackPanel>
            <Button Name="btnDodajPacjenta" Content="Dodaj Pacjenta" Margin="10" Visibility="Collapsed" Click="btnDodajPacjenta_Click"/>
            <Button Name="btnDodajPracownika" Content="Dodaj Pracownika" Margin="10" Click="btnDodajPracownika_Click" Visibility="Collapsed"/>
            <Button Name="btnUmowWizyte" Content="Umów Wizytę" Margin="10" Click="btnUmowWizyte_Click" Visibility="Collapsed"/>
            <Button Name="btnWyswietlPacjentow" Content="Wyświetl Pacjentów" Margin="10" Click="btnWyswietlPacjentow_Click" Visibility="Collapsed"/>
            <Button Name="btnWyswietlPracownikow" Content="Wyświetl Pracowników" Margin="10" Click="btnWyswietlPracownikow_Click" Visibility="Collapsed"/>
            <Button Name="btnWyswietlWizyty" Content="Wyświetl Wizyty" Margin="10"/>
            <Button Name="btnWyswietlRaporty" Content="Wyświetl Raporty" Margin="10" Visibility="Collapsed"/>
        </StackPanel>
    </Grid>
</Window>
